<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Elements_task.horoskoop">
    <ContentPage.Content>
        <StackLayout>
            <DatePicker x:Name="date" DateSelected="changehoros" MinimumDate="01/01/2020"
                    MaximumDate="12/31/2020"
                    Date="11/11/2020" />
            <Image x:Name="horos" Source="question.png"/>
            <Entry x:Name="info" Text="Who are you?"/>
            <Label x:Name="change" IsVisible="false"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>